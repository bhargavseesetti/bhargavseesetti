#include <stdio.h>

int main(void) 
{
  int a,b,c;
  printf("enter the angle of a triangle (in degrees);:\n");
  printf("a= " );
  scanf("%d",&a);
  printf("b= ");
  scanf("%d",&b);
  printf("c= ");
  scanf("%d",&c);

  if(a+b+c !=180)
  printf("invalid angle\n");
  else if (a==b && b==c)
  printf("equilaterial triangle\n");
  else if(a==b || b==c || c==a)
  printf ("isoseless triangle\n");
  else 
  printf("scalesne triangle\n");
  return 0;
}
